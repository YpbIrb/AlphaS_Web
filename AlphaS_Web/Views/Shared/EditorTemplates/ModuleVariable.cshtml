@model AlphaS_Web.Models.ModuleVariable
<div class="row">
    <hr />
    <div class="col-md-12">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="VariableName" class="control-label"></label>
            <input asp-for="VariableName" class="form-control" />
            <span asp-validation-for="VariableName" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="VariableDefaultValue" class="control-label"></label>
            <input asp-for="VariableDefaultValue" class="form-control" />
            <span asp-validation-for="VariableDefaultValue" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="VariableDescription" class="control-label"></label>
            <input asp-for="VariableDescription" class="form-control" />
            <span asp-validation-for="VariableDescription" class="text-danger"></span>
        </div>
    </div>
</div>