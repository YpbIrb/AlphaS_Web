@model AlphaS_Web.Models.ExperimentPreset

@{
    ViewData["Title"] = "Details";
}

<h1>Информация о шаблоне эксперимента</h1>

<div>
    <h4>ExperimentPreset</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.PresetName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PresetName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Modules)
        </dt>

        @foreach (var module in Model.Modules)
        {
            @Html.DisplayFor(model => module)
        }



    </dl>
</div>
<div>
    <a asp-action="Index">Назад к списку</a>
</div>
