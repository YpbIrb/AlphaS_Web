@model IEnumerable<AlphaS_Web.Models.ExperimentPreset>

@{
    ViewData["Title"] = "Index";
}

<h1>Шаблоны экспериментов</h1>

<p>
    <a asp-action="Create">Создать шаблон</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PresetName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PresetName)
            </td>
            <td>
                @Html.ActionLink("Детали", "Details", new {  id=item.PresetName  }) |
            </td>
        </tr>
}
    </tbody>
</table>
