@model AlphaS_Web.Models.Experiment

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Experiment</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ExperimentId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ExperimentId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OperatorId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OperatorId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FirstParticipant)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FirstParticipant)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.SecondParticipant)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SecondParticipant)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StartTime)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StartTime)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FinishTime)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FinishTime)
        </dd>

        @Html.DisplayNameFor(model => model.Modules)

        @foreach (ModuleInExperiment module in Model.Modules)
        {
            @Html.DisplayFor(model => module)
        }


    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new {  id = Model.ExperimentId }) |
    <a asp-action="Index">Back to List</a>
</div>
